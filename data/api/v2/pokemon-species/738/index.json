    "flavor_text_entries": [
        {
            "flavor_text": "\u7528\u4f4d\u4e8e\u8179\u90e8\u7684\u53d1\u7535\u5668\u5b98\r\n\u53d1\u51fa\u7535\u529b\u3002\u5373\u4fbf\u662f\u9e1f\u5b9d\u53ef\u68a6\r\n\u4e5f\u80fd\u88ab\u5b83\u7528\u7535\u51fb\u5149\u675f\u538b\u5236\u3002",
